{
  "name": "plant-monitor-server",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "startWebpack": "node backend-dev.js",
    "startDevServer": "nodemon --exec './node_modules/.bin/babel-node --inspect=0.0.0.0 src/bin/www'",
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "testAll": "./node_modules/.bin/mocha --require @babel/register tests/*.js ",
    "build": "node_modules/.bin/babel src -d dist",
    "startProd": "node dist/bin/www",
    "sequelize-cli": "./node_modules/.bin/sequelize-cli",
    "dbMigrateAll": "./node_modules/.bin/sequelize-cli db:migrate",
    "dbSeedAll": "./node_modules/.bin/sequelize-cli db:seed:all",
    "dbUndoMigrateAll": "./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "console": "DB_HOST=localhost POSTGRES_DB=home_automation_dev POSTGRES_USER= POSTGRES_PASSWORD= ./node_modules/.bin/babel-node --experimental-repl-await ./console",
    "eslintFix": "./node_modules/.bin/eslint --fix"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.1",
    "morgan": "~1.9.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "rate-limiter-flexible": "^2.1.13",
    "redis": "^2.8.0",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.0",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "proxyquire": "^2.1.0",
    "sequelize-mock": "^0.10.2",
    "shebang-loader": "^0.0.1",
    "sinon": "^7.3.2",
    "webpack": "^4.30.0"
  }
}
